<application xmlns:soa="http://www.oracle.com/soa/rest" xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:tns="http://www.mozabank.org/TDOpen" xmlns="http://wadl.dev.java.net/2009/02" xmlns:ns0="http://www.mozabank.org/CAPP">
   <doc title="TDOpen_BS">RestReference</doc>
   <grammars>
      <xsd:schema>
            <xsd:import namespace="http://www.mozabank.org/TDOpen" schemaLocation="TD_OPEN.xsd"/>
        </xsd:schema>
      <xsd:schema>
            <xsd:import namespace="http://www.mozabank.org/CAPP" schemaLocation="Schema/CAPP.xsd"/>
        </xsd:schema>
   </grammars>
   <resources>
      <resource path="/">
         <method name="POST" soa:wsdlOperation="TDOpen">
            <request>
               <representation mediaType="application/json" element="cns:CAPPRequest" xmlns:cns="http://www.mozabank.org/CAPP"/>
            </request>
            <response status="200">
               <representation mediaType="application/json" element="cns:CAPPResponse" xmlns:cns="http://www.mozabank.org/CAPP"/>

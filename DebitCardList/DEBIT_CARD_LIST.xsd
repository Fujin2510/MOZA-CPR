<?xml version="1.0" encoding="UTF-8"?>
<xsd:schema xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:nxsd="http://xmlns.oracle.com/pcbpel/nxsd"
            targetNamespace="http://www.mozabanca.org/ccar/obdx" xmlns="http://www.mozabanca.org/ccar/obdx"
            elementFormDefault="qualified" attributeFormDefault="unqualified" nxsd:version="JSON" nxsd:encoding="UTF-8">
  <!-- Root Elements -->
  <xsd:element name="DebitCardListRequest">
    <xsd:complexType>
      <xsd:sequence>
        <xsd:element name="dictionaryArray" type="xsd:string" minOccurs="0" nillable="true" maxOccurs="unbounded"/> 
        <xsd:element name="referenceNo" type="xsd:string"/>
        <xsd:element name="interfaceId" type="xsd:string"/>
        <xsd:element name="entity" type="xsd:string"/>
        <xsd:element name="headersMap">
          <xsd:complexType>
            <xsd:sequence>
              <!-- empty map represented as empty sequence -->
            </xsd:sequence>
          </xsd:complexType>
        </xsd:element>
        <xsd:element name="queryMap" type="xsd:string" minOccurs="0" nillable="true"/>
        <xsd:element name="hostName" type="xsd:string"/>
        <xsd:element name="partyId" type="xsd:string" minOccurs="0"/>
        <xsd:element name="accountId" type="xsd:string"/>
        <xsd:element name="branchCode" type="xsd:string"/>
      </xsd:sequence>
    </xsd:complexType>
  </xsd:element>
  <xsd:element name="dictionaryArray">
    <xsd:complexType>
      <xsd:sequence>
        <xsd:element name="nameValuePairArray" maxOccurs="unbounded">
          <xsd:complexType>
            <xsd:sequence>
              <xsd:element name="name" type="xsd:string"/>
              <xsd:element name="value" type="xsd:string"/>
              <xsd:element name="genericName" type="xsd:string"/>
              <xsd:element name="datatype" type="xsd:string"/>
            </xsd:sequence>
          </xsd:complexType>
        </xsd:element>
      </xsd:sequence>
    </xsd:complexType>
  </xsd:element>

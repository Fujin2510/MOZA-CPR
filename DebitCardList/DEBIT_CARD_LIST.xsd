                            <xsd:complexType>
                              <xsd:sequence>
                                <xsd:element name="currency" type="xsd:string"/>
                                <xsd:element name="amount" type="xsd:decimal"/>
                              </xsd:sequence>
                            </xsd:complexType>
                          </xsd:element>
                          <xsd:element name="count" type="xsd:integer"/>
                          <xsd:element name="limitType" type="xsd:string"/>
                          <xsd:element name="maxLimitAmount">
                            <xsd:complexType>
                              <xsd:sequence>
                                <xsd:element name="currency" type="xsd:string"/>
                                <xsd:element name="amount" type="xsd:decimal"/>
                              </xsd:sequence>
                            </xsd:complexType>
                          </xsd:element>
                        </xsd:sequence>
                      </xsd:complexType>
                    </xsd:element>
                    <xsd:element name="totalAmountLimit">
                      <xsd:complexType>
                        <xsd:sequence>
                          <xsd:element name="currency" type="xsd:string"/>
                          <xsd:element name="amount" type="xsd:decimal"/>
                        </xsd:sequence>
                      </xsd:complexType>
                    </xsd:element>
                    <xsd:element name="totalAmountMaxLimit">
                      <xsd:complexType>
                        <xsd:sequence>
                          <xsd:element name="currency" type="xsd:string"/>

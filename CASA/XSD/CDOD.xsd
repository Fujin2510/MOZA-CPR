<xsd:element name="CDOD_O_0008" type="xsd:string"/>
<xsd:element name="CDOD_O_00081" type="xsd:string"/>
<xsd:element name="CDOD_O_00082" type="xsd:string"/>
<xsd:element name="CDOD_O_0009" type="xsd:string"/>
<xsd:element name="CDOD_O_0010" type="xsd:string"/>
<xsd:element name="CDOD_O_0011" type="xsd:string"/>
<xsd:element name="CDOD_O_0012" type="xsd:string"/>
<xsd:element name="CDOD_O_0013" type="xsd:string"/>
<xsd:element name="CDOD_O_0014" type="xsd:string"/>
<xsd:element name="CDOD_O_0015" type="xsd:string"/>
<xsd:element name="CDOD_O_0016" type="xsd:string"/>
<xsd:element name="CDOD_O_0017" type="xsd:string"/>
<xsd:element name="CDOD_O_0018" type="xsd:string"/>
<xsd:element name="CDOD_O_0019" type="xsd:string"/>
<xsd:element name="CDOD_O_00191" type="xsd:string"/>
<xsd:element name="CDOD_O_00192" type="xsd:string"/>
<xsd:element name="CDOD_O_0020" type="xsd:string"/>
<xsd:element name="CDOD_O_0021" type="xsd:string"/>
<xsd:element name="CDOD_O_0022" type="xsd:string"/>
<xsd:element name="CDOD_O_0023" type="xsd:string"/>
<xsd:element name="CDOD_O_0024" type="xsd:string"/>
<xsd:element name="CDOD_O_0025" type="xsd:string"/>
<xsd:element name="CDOD_O_0026" type="xsd:string"/>
<xsd:element name="CDOD_O_0027" type="xsd:string"/>
</xsd:sequence>
</xsd:complexType>
 
  <!-- Error Message Type -->
<xsd:complexType name="ErrorMessageType">
<xsd:sequence>
<xsd:element name="messages" type="xsd:string" minOccurs="0" maxOccurs="unbounded" nillable="true"/>
</xsd:sequence>
</xsd:complexType>
 
  <!-- Validation Type
<xsd:complexType name="ValidationType">
<xsd:sequence>
<xsd:element name="confirmationKey" type="xsd:string" minOccurs="0" nillable="true"/>
</xsd:sequence>
</xsd:complexType>  -->
 
</xsd:schema>
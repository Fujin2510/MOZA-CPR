          <xsd:sequence>
            <xsd:element name="currency" type="xsd:string" minOccurs="0" nillable="true"/>
            <xsd:element name="amount" type="xsd:decimal" minOccurs="0" nillable="true"/>
          </xsd:sequence>
        </xsd:complexType>
      </xsd:element>
      <xsd:element name="interestType" type="xsd:string" minOccurs="0" nillable="true"/>
      <xsd:element name="interestRate" type="xsd:decimal" minOccurs="0" nillable="true"/>
      <xsd:element name="openingDate" type="xsd:string" minOccurs="0" nillable="true"/>
      <xsd:element name="relationshipType" type="xsd:string" minOccurs="0" nillable="true"/>
      <xsd:element name="accountModule" type="xsd:string" minOccurs="0" nillable="true"/>
      <xsd:element name="sortCode" type="xsd:string" minOccurs="0" nillable="true"/>
      <xsd:element name="relation" type="xsd:string" minOccurs="0" nillable="true"/>
      <xsd:element name="moduleType" type="xsd:string" minOccurs="0" nillable="true"/>
      <xsd:element name="iban" type="xsd:string" minOccurs="0" nillable="true"/>
    </xsd:sequence>
  </xsd:complexType>
</xsd:schema>
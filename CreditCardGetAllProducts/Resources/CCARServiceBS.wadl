<application xmlns:soa="http://www.oracle.com/soa/rest" xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:tns="http://www.mozabank.org/CCARCreditCard" xmlns="http://wadl.dev.java.net/2009/02">
   <doc title="CCARServiceBS">RestReference</doc>
   <grammars>
      <xsd:schema>
            <xsd:import namespace="http://www.mozabank.org/CCARCreditCard" schemaLocation="XSD/CCAR.xsd"/>
        </xsd:schema>
   </grammars>
   <resources>
      <resource path="/OP_CALL_ENC">
         <method name="GET" soa:wsdlOperation="CCAR">
            <request soa:paramElement="pns:RequestString" xmlns:pns="http://www.mozabank.org/CCARCreditCard">
               <param name="request" style="query" soa:expression="$msg.request/tns:request" type="xsd:string"/>
            </request>
            <response status="200">
               <representation mediaType="application/xml" element="cns:Response" xmlns:cns="http://www.mozabank.org/CCARCreditCard"/>
               <representation mediaType="application/json" element="cns:Response" xmlns:cns="http://www.mozabank.org/CCARCreditCard"/>
            </response>
         </method>
      </resource>
   </resources>
</application>
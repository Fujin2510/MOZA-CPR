<application xmlns:soa="http://www.oracle.com/soa/rest" xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:ns0="http://www.mozabanca.org/obdx/CTFO" xmlns="http://wadl.dev.java.net/2009/02">
   <doc title="CTFO-MSB">RestReference</doc>
   <grammars>
      <xsd:schema>
            <xsd:import namespace="http://www.mozabanca.org/obdx/CTFO" schemaLocation="../CTFO.xsd"/>
        </xsd:schema>
   </grammars>
   <resources>
      <resource path="/OP_CALL_ENC">
         <method name="GET" soa:wsdlOperation="CTFOGET">
            <request soa:paramElement="pns:RequestEncoded" xmlns:pns="http://www.mozabanca.org/obdx/CTFO">
               <param name="request" style="query" soa:expression="$msg.request/ns0:requestString" type="xsd:string"/>
            </request>
            <response status="200">
               <representation mediaType="application/json" element="cns:Response" xmlns:cns="http://www.mozabanca.org/obdx/CTFO"/>
               <representation mediaType="application/xml" element="cns:Response" xmlns:cns="http://www.mozabanca.org/obdx/CTFO"/>
            </response>
         </method>
      </resource>
   </resources>
</application>
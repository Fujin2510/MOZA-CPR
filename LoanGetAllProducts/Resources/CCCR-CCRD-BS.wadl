<application xmlns:soa="http://www.oracle.com/soa/rest" xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:tns="http://www.mozabank.org/CCCR_CCRD" xmlns="http://wadl.dev.java.net/2009/02">
   <doc title="CCCR-CCRD-BS">RestReference</doc>
   <grammars>
      <xsd:schema>
            <xsd:import namespace="http://www.mozabank.org/CCCR_CCRD" schemaLocation="Schema/CCCR-CCRD.xsd"/>
        </xsd:schema>
   </grammars>
   <resources>
      <resource path="/OP_CALL_ENC">
         <method name="GET" soa:wsdlOperation="CCCR-CCRD">
            <request soa:paramElement="pns:CCCREncodedRequest" xmlns:pns="http://www.mozabank.org/CCCR_CCRD">
               <param name="request" style="query" soa:expression="$msg.request/tns:RequestString" type="xsd:string"/>
            </request>
            <response status="200">
               <representation mediaType="application/xml" element="cns:CCCRResponse" xmlns:cns="http://www.mozabank.org/CCCR_CCRD"/>
               <representation mediaType="application/json" element="cns:CCCRResponse" xmlns:cns="http://www.mozabank.org/CCCR_CCRD"/>
            </response>
         </method>
      </resource>
   </resources>
</application>
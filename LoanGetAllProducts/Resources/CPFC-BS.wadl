<application xmlns:soa="http://www.oracle.com/soa/rest" xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:ns0="http://www.mozabanca.org/cpfc" xmlns="http://wadl.dev.java.net/2009/02">
   <doc title="CPFC-BS">RestReference</doc>
   <grammars>
      <xsd:schema>
            <xsd:import namespace="http://www.mozabanca.org/cpfc" schemaLocation="Schema/CPFC.xsd"/>
        </xsd:schema>
   </grammars>
   <resources>
      <resource path="/OP_CALL_ENC">
         <method name="GET" soa:wsdlOperation="CPFC">
            <request soa:paramElement="pns:CPFCEncodedRequest" xmlns:pns="http://www.mozabanca.org/cpfc">
               <param name="request" style="query" soa:expression="$msg.request/ns0:RequestString" type="xsd:string"/>
            </request>
            <response status="200">
               <representation mediaType="application/xml" element="cns:CPFCResponse" xmlns:cns="http://www.mozabanca.org/cpfc"/>
               <representation mediaType="application/json" element="cns:CPFCResponse" xmlns:cns="http://www.mozabanca.org/cpfc"/>
            </response>
         </method>
      </resource>
   </resources>
</application>
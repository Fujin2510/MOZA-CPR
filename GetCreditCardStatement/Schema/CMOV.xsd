      <xsd:element name="CMOV_O_0011_0018" type="xsd:string"  minOccurs="0"/>
      <xsd:element name="CMOV_O_0011_0019" type="xsd:string"  minOccurs="0"/>
    </xsd:sequence>
  </xsd:complexType>

  <!-- Error message -->
  <xsd:complexType name="ErrorMessageType">
    <xsd:sequence>
      <xsd:element name="messages" type="xsd:string" minOccurs="0" maxOccurs="unbounded"/>
    </xsd:sequence>
  </xsd:complexType>

</xsd:schema>
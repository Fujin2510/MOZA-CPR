<?xml version="1.0" encoding="UTF-8"?>
<xsd:schema xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:nxsd="http://xmlns.oracle.com/pcbpel/nxsd"
              xmlns:tns="http://www.mozabank.org/CMOV"
              targetNamespace="http://www.mozabank.org/CMOV" elementFormDefault="qualified"
              attributeFormDefault="unqualified"  
             nxsd:version="JSON" nxsd:encoding="UTF-8">

<!-- Root Elements -->
        <xsd:element name="RequestEncoded">
        <xsd:complexType>
        <xsd:sequence>
        <xsd:element name="request" type="xsd:string"  minOccurs="0"/>
        </xsd:sequence>
        </xsd:complexType>
        </xsd:element>
        <xsd:element name="Response" type="tns:CMOVResponseType"/>
          
  <!-- Request -->
  <xsd:element name="CMOVRequest" type="tns:CMOVRequestType"/>
  <xsd:complexType name="CMOVRequestType">
    <xsd:sequence>
      <xsd:element name="user" type="xsd:string"  minOccurs="0"/>
      <xsd:element name="password" type="xsd:string"  minOccurs="0"/>
      <xsd:element name="origin" type="xsd:string"  minOccurs="0"/>
      <xsd:element name="channelCode" type="xsd:string"  minOccurs="0"/>
      <xsd:element name="version" type="xsd:string"  minOccurs="0"/>
      <xsd:element name="licenceKey" type="xsd:string"  minOccurs="0"/>
      <xsd:element name="sessionId" type="xsd:string"  minOccurs="0"/>
      <xsd:element name="transactionCode" type="xsd:string"  minOccurs="0"/>
      <xsd:element name="operationData" type="tns:CMOVRequestOperationDataType"/>
    </xsd:sequence>
  </xsd:complexType>

  <xsd:complexType name="CMOVRequestOperationDataType">
    <xsd:sequence>
      <xsd:element name="CMOV_I_0001" type="xsd:string"  minOccurs="0"/>
      <xsd:element name="CMOV_I_0002" type="xsd:string"  minOccurs="0"/>
      <xsd:element name="CMOV_I_0003" type="xsd:string"  minOccurs="0"/>
      <xsd:element name="CMOV_I_0004" type="xsd:string"  minOccurs="0"/>
      <xsd:element name="CMOV_I_0005" type="xsd:string"  minOccurs="0"/>
      <xsd:element name="CMOV_I_0006" type="xsd:string"  minOccurs="0"/>
      <xsd:element name="CMOV_I_0007" type="xsd:string"  minOccurs="0"/>
      <xsd:element name="CMOV_I_9998" type="xsd:string"  minOccurs="0"/>
      <xsd:element name="CMOV_I_9999" type="xsd:string"  minOccurs="0"/>
    </xsd:sequence>
  </xsd:complexType>

  <!-- Response -->
  <xsd:element name="CMOVResponse" type="tns:CMOVResponseType"/>
  <xsd:complexType name="CMOVResponseType">
    <xsd:sequence>
      <xsd:element name="transactionCode" type="xsd:string"  minOccurs="0"/>
      <xsd:element name="operationData" type="tns:CMOVResponseOperationDataType"/>
      <xsd:element name="channelOperationLogKey" type="xsd:string"  minOccurs="0"/>
      <xsd:element name="coreLogKey" type="xsd:string"  minOccurs="0"/>
      <xsd:element name="coreCancelationCode" type="xsd:string"  minOccurs="0"/>
      <xsd:element name="errorCode" type="xsd:integer"/>
      <xsd:element name="errorMessage" type="tns:ErrorMessageType"/>
      <xsd:element name="layoutVersion" type="xsd:string"  minOccurs="0"/>
      <xsd:element name="user" type="xsd:string"  minOccurs="0"/>
      <xsd:element name="password" type="xsd:string"  minOccurs="0"/>
      <xsd:element name="station" type="xsd:string"  minOccurs="0"/>
      <xsd:element name="licenceKey" type="xsd:string"  minOccurs="0"/>
      <xsd:element name="encryption" type="xsd:string"  minOccurs="0"/>
      <xsd:element name="code" type="xsd:string"  minOccurs="0"/>
      <xsd:element name="version" type="xsd:string"  minOccurs="0"/>
      <xsd:element name="sessionId" type="xsd:string"  minOccurs="0"/>
      <xsd:element name="language" type="xsd:string"  minOccurs="0"/>
      <xsd:element name="channelCode" type="xsd:string"  minOccurs="0"/>
      <xsd:element name="origin" type="xsd:string"  minOccurs="0"/>
</xsd:sequence>
</xsd:complexType>

  <!-- operationData response -->
  <xsd:complexType name="CMOVResponseOperationDataType">
    <xsd:sequence>
      <xsd:element name="CMOV_O_0001" type="xsd:string"  minOccurs="0"/>
      <xsd:element name="CMOV_O_0002" type="xsd:string"  minOccurs="0"/>
      <xsd:element name="CMOV_O_0003" type="xsd:string"  minOccurs="0"/>
      <xsd:element name="CMOV_O_0004" type="xsd:string"  minOccurs="0"/>
      <xsd:element name="CMOV_O_0005" type="xsd:string"  minOccurs="0"/>

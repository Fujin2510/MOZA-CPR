      <xsd:element name="CMOV_O_00081" type="xsd:string"  minOccurs="0"/>
      <xsd:element name="CMOV_O_00082" type="xsd:string"  minOccurs="0"/>
      <xsd:element name="CMOV_O_00083" type="xsd:string"  minOccurs="0"/>
      <xsd:element name="CMOV_O_00084" type="xsd:string"  minOccurs="0"/>
      <xsd:element name="CMOV_O_00085" type="xsd:string"  minOccurs="0"/>
      <xsd:element name="CMOV_O_0009" type="xsd:string"  minOccurs="0"/>
      <xsd:element name="CMOV_O_0010" type="xsd:string"  minOccurs="0"/>
      <xsd:element name="CMOV_O_0011" type="tns:CMOVDetailListType" maxOccurs="unbounded"/>
    </xsd:sequence>
  </xsd:complexType>

  <!-- List item details -->
  <xsd:complexType name="CMOVDetailListType">
    <xsd:sequence>
      <xsd:element name="CMOV_O_0011_0001" type="xsd:string"  minOccurs="0"/>
      <xsd:element name="CMOV_O_0011_00011" type="xsd:string"  minOccurs="0"/>
      <xsd:element name="CMOV_O_0011_0002" type="xsd:string"  minOccurs="0"/>
      <xsd:element name="CMOV_O_0011_0003" type="xsd:string"  minOccurs="0"/>
      <xsd:element name="CMOV_O_0011_0004" type="xsd:string"  minOccurs="0"/>
      <xsd:element name="CMOV_O_0011_0005" type="xsd:string"  minOccurs="0"/>
      <xsd:element name="CMOV_O_0011_0006" type="xsd:integer" minOccurs="0"/>
      <xsd:element name="CMOV_O_0011_0007" type="xsd:string"  minOccurs="0"/>
      <xsd:element name="CMOV_O_0011_0008" type="xsd:string"  minOccurs="0"/>

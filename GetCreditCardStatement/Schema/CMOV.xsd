      <xsd:element name="CMOV_O_00052" type="xsd:string"  minOccurs="0"/>
      <xsd:element name="CMOV_O_0006" type="xsd:string"  minOccurs="0"/>
      <xsd:element name="CMOV_O_0007" type="xsd:string"  minOccurs="0"/>
      <xsd:element name="CMOV_O_00071" type="xsd:string"  minOccurs="0"/>
      <xsd:element name="CMOV_O_00072" type="xsd:string"  minOccurs="0"/>
      <xsd:element name="CMOV_O_0008" type="xsd:string"  minOccurs="0"/>
      <xsd:element name="CMOV_O_00086" type="xsd:string"  minOccurs="0"/>

<application xmlns:soa="http://www.oracle.com/soa/rest" xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:tns="http://www.mozabank.org/CASA_STATEMENT_ITEM_LIST" xmlns="http://wadl.dev.java.net/2009/02">
   <doc title="CMOV">RestReference</doc>
   <grammars>
      <xsd:schema>
            <xsd:import namespace="http://www.mozabank.org/CMOV" schemaLocation="../Schema/CMOV.xsd"/>
        </xsd:schema>
   </grammars>
   <resources>
      <resource path="/OP_CALL_ENC">
         <method name="GET" soa:wsdlOperation="CMOV">
            <request soa:paramElement="pns:RequestEncoded" xmlns:pns="http://www.mozabank.org/CMOV">
               <param name="request" style="query" soa:expression="$msg.request/tns:requestString" type="xsd:string"/>
            </request>
            <response status="200">
               <representation mediaType="application/xml" element="cns:CMOVResponse" xmlns:cns="http://www.mozabank.org/CMOV"/>
               <representation mediaType="application/json" element="cns:CMOVResponse" xmlns:cns="http://www.mozabank.org/CMOV"/>
            </response>
         </method>
      </resource>
   </resources>
</application>
<application xmlns:soa="http://www.oracle.com/soa/rest" xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:tns="http://www.mozabank.org/branchCode" xmlns="http://wadl.dev.java.net/2009/02">
   <doc title="BranchCodeService">RestService</doc>
   <grammars>
      <xsd:schema>
            <xsd:import namespace="http://www.mozabank.org/branchCode" schemaLocation="../Schema/OBDXSchema/BRANCH_CODE.xsd"/>
        </xsd:schema>
   </grammars>
   <resources>
      <resource path="/branchcode">
         <method name="POST" soa:wsdlOperation="BranchCode">
            <request>
               <representation mediaType="application/xml" element="cns:Request" xmlns:cns="http://www.mozabank.org/branchCode"/>
               <representation mediaType="application/json" element="cns:Request" xmlns:cns="http://www.mozabank.org/branchCode"/>
            </request>
            <response status="200">
               <representation mediaType="application/xml" element="cns:Response" xmlns:cns="http://www.mozabank.org/branchCode"/>
               <representation mediaType="application/json" element="cns:Response" xmlns:cns="http://www.mozabank.org/branchCode"/>
            </response>
         </method>
      </resource>
   </resources>
</application>
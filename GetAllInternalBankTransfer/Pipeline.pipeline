                                    <con:xqueryText xmlns:con="http://www.bea.com/wli/sb/stages/config"><![CDATA[(: Assign fromDate and toDate as strings formatted yyyymmdd :)
let $from := current-date() + xs:yearMonthDuration("P6M")
let $yearFrom := string(year-from-date($from))
let $monthFrom := month-from-date($from)
let $dayFrom := day-from-date($from)
let $monthFromStr := if ($monthFrom lt 10) then concat('0', $monthFrom) else string($monthFrom)
let $dayFromStr := if ($dayFrom lt 10) then concat('0', $dayFrom) else string($dayFrom)
let $fromDate := concat($yearFrom, $monthFromStr, $dayFromStr)

let $to := current-date()
let $yearTo := string(year-from-date($to))
let $monthTo := month-from-date($to)
let $dayTo := day-from-date($to)
let $monthToStr := if ($monthTo lt 10) then concat('0', $monthTo) else string($monthTo)
let $dayToStr := if ($dayTo lt 10) then concat('0', $dayTo) else string($dayTo)

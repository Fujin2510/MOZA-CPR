<application xmlns:soa="http://www.oracle.com/soa/rest" xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:tns="http://www.mozabank.org/CreditAccountDetails" xmlns="http://wadl.dev.java.net/2009/02">
   <doc title="CCRDServiceBS">RestReference</doc>
   <grammars>
      <xsd:schema>
            <xsd:import namespace="http://www.mozabank.org/CreditAccountDetails" schemaLocation="CCRD.xsd"/>
        </xsd:schema>
   </grammars>
   <resources>
      <resource path="/OP_CALL_ENC">
         <method name="GET" soa:wsdlOperation="CCRD">
            <request soa:paramElement="pns:CCRDRequestString" xmlns:pns="http://www.mozabank.org/CreditAccountDetails">
               <param name="request" style="query" soa:expression="$msg.request/tns:RequestEncoded" type="xsd:string"/>
            </request>
            <response status="200">
               <representation mediaType="application/json" element="cns:CreditAccountDetailsResponse" xmlns:cns="http://www.mozabank.org/CreditAccountDetails"/>
            </response>
         </method>
      </resource>
   </resources>
</application>